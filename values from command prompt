#include<stdio.h>
#include<unistd.h>
void main()
{
int n,a[10];
printf("Enter the total number to be entered\n");
scanf("%d",&n);
for(int i=0;i<n;i++)
{
printf("Enter number %d ",i);
scanf("%d",&a[i]);
}
printf("NUMBERS ENTERED ARE : \n");
for(int i=0;i<n;i++)
{
printf("\n");
printf("%d",a[i]);

}
}
